{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "linden",
  "name": "Paperjet",
  "colors": {
    "primary": "#FDFBF7",
    "light": "#FDFBF7",
    "dark": "#FDFBF7"
  },
  "appearance": {
    "default": "light",
    "strict": true
  },
  "favicon": "/favicon.ico",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Get Started",
            "pages": ["index", "quickstart", "development"]
          },
          {
            "group": "User Guide",
            "pages": ["pages/user-guide/workflows"]
          },
          {
            "group": "Adminstrator Guide",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "pages": [
          "rest/workflows/create-workflow",
          "rest/workflows/update-workflow",
          "rest/workflows/delete-workflow",
          "rest/workflows/list-workflows"
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://mintlify.com/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Community",
          "href": "https://getpaperjet.com/discord",
          "icon": "slack"
        },
        {
          "anchor": "Blog",
          "href": "https://getpaperjet.com/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:contact@getpaperjet.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://app.getpaperjet.com"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/nomoredevwork",
      "github": "https://github.com/mlnativeai/paperjet",
      "linkedin": "https://linkedin.com/company/paperjet"
    }
  }
}
