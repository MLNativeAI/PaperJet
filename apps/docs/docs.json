        "pages": [
          {
            "group": "Workflows",
            "pages": [
              "api-reference/get-apiv1workflows",
              "api-reference/post-apiv1workflows",
              "api-reference/post-apiv1workflows-execute",
              "api-reference/get-apiv1workflows-1",
