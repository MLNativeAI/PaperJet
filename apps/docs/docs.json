{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Paperjet",
  "colors": {
    "primary": "#a67c52",
    "light": "#e2d8c3",
    "dark": "#5c4d3f"
  },
  "background": {
    "color": {
      "light": "#FDFBF7",
      "dark": "#4A3B33"
    }
  },
  "appearance": {
    "default": "light",
    "strict": true
  },
  "favicon": "/favicon.ico",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Get Started",
            "pages": ["index"]
          },
          {
            "group": "User Guide",
            "pages": ["pages/user/workflows"]
          },
          {
            "group": "Adminstrator Guide",
            "pages": ["pages/admin/quickstart", "pages/admin/deployment"]
          }
        ]
      },
      {
        "tab": "API Reference",
        "pages": [
          "rest/workflows/create-workflow",
          "rest/workflows/update-workflow",
          "rest/workflows/delete-workflow",
          "rest/workflows/list-workflows"
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Support",
          "href": "https://getpaperjet.com/support",
          "icon": "question"
        },
        {
          "anchor": "Github",
          "href": "https://github.com/mlnativeai/paperjet",
          "icon": "github"
        },
        {
          "anchor": "Community",
          "href": "https://discord.gg/5AF2wDKWBT",
          "icon": "discord"
        },
        {
          "anchor": "Blog",
          "href": "https://getpaperjet.com/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo-docs-no-bg.png",
    "dark": "/logo-docs-no-bg.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:contact@getpaperjet.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://app.getpaperjet.com"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/nomoredevwork",
      "github": "https://github.com/mlnativeai/paperjet",
      "linkedin": "https://linkedin.com/company/paperjet"
    }
  }
}
