              "api-reference/delete-apiv1api-keys"
            ]
          },
          {
            "group": "Admin",
            "pages": [
              "api-reference/get-apiv1adminruntime-config",
              "api-reference/post-apiv1adminruntime-config",
              "api-reference/get-apiv1adminmodels",
              "api-reference/post-apiv1adminmodelsadd",
